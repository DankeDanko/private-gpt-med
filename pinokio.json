{
  "name": "privategpt",
  "description": "PrivateGPT oficial con LM Studio local",
  "requirements": [
    {
      "type": "python",
      "version": ">=3.10"
    },
    {
      "type": "pip",
      "packages": [
        "-r requirements.txt"
      ]
    }
  ],
  "scripts": {
    "start": [
      "python -m privategpt.webserver"
    ]
  },
  "expose": [
    {
      "type": "web",
      "name": "WebUI",
      "url": "http://localhost:8001"
    }
  ]
}
